{
  "compilerOptions": {
    "target": "es2017",
    "module": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "strict": true,
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@faasjs-starter/types": ["../../types"]
    }
  },
  "references": [{
    "path": "../types"
  }]
}
