{
  "name": "@faasjs-starter/www",
  "private": "true",
  "version": "0.0.0",
  "dependencies": {
    "@ant-design/icons": "*",
    "@faasjs/eslint-config-react": "^0.0.2-beta.353",
    "@faasjs/react": "^0.0.2-beta.353",
    "swc-loader": "*",
    "antd": "*",
    "html-loader": "*",
    "html-webpack-plugin": "*",
    "less-loader": "*",
    "lodash": "*",
    "react": "*",
    "react-dom": "*",
    "style-loader": "*",
    "url-loader": "*",
    "uuid": "*",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "devDependencies": {
    "@swc/jest": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@types/jest": "*",
    "@types/lodash": "*",
    "@types/node": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "@types/uuid": "*",
    "jest": "*"
  },
  "scripts": {
    "dev": "webpack-cli serve --progress --config .webpack/dev.config.js",
    "test": "jest",
    "ci": "jest --coverage --ci"
  },
  "eslintConfig": {
    "extends": [
      "@faasjs/react"
    ]
  },
  "jest": {
    "rootDir": ".",
    "transform": {
      ".(jsx|tsx?)": "@swc/jest"
    },
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "**/*.tsx"
    ],
    "testRegex": "/*\\.test\\.tsx?$",
    "moduleNameMapper": {
      "^libs(.*)$": "<rootDir>/src/libs$1"
    },
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ]
  }
}
